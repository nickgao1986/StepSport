<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:left="-1dp">
        <shape>
            <solid android:color="@color/us_palette_white"/>
            <stroke
                android:width="@dimen/default_button_stroke_width"
                android:color="@color/us_palette_black_lighter_1"/>
            <corners
                android:bottomLeftRadius="0dp"
                android:bottomRightRadius="@dimen/default_button_radius"
                android:topLeftRadius="0dp"
                android:topRightRadius="@dimen/default_button_radius"/>
            <padding
                android:bottom="@dimen/default_button_padding"
                android:left="@dimen/default_button_padding"
                android:right="@dimen/default_button_padding"
                android:top="@dimen/default_button_padding"/>
        </shape>
    </item>
</layer-list>
